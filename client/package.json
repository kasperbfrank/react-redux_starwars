{
  "name": "react-redux_starwars_api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "start:prod": "webpack-dev-server --inline --host 0.0.0.0 --port 80",
    "serve": "webpack-dev-server --hot --inline --content-base ./src --host 0.0.0.0",
    "docker:run-prod": "docker-compose up",
    "docker:run-dev" : "docker-compose build && docker-compose -f dev.docker-compose.yml up",
    "docker:run-full-dev": "docker-compose -f ../api/docker-compose.yml up && yarn docker:run-dev"
  },
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "node-sass": "^4.5.3",
    "react-hot-loader": "^3.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.1"
  }
}
